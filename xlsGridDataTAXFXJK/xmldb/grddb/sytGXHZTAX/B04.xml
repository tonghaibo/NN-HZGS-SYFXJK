select todept,sum(c1) c1,sum(c2) c2,sum(c3) c3,sum(c4) c4,sum(c5) c5,sum(c6) c6 from (
select todept,decode(newsubtyp,1,cnt) c1,decode(newsubtyp,2,cnt) c2,decode(newsubtyp,3,cnt) c3,decode(newsubtyp,4,cnt) c4,decode(newsubtyp,5,cnt) c5,decode(newsubtyp,6,cnt) c6 from (
