<ROW num="0" ><ID >MAIN</ID><NAME ></NAME><DATDSC >select rownum,id,name,lawman,somny,taxmay,sdtaxmny,yjje,yjse,taxman,addr from (
select id,name,taxman,somny,taxmay,sdtaxmny,addr,lawman,yjje,yjse from(
select a.id,a.name,c.name taxman,round(nvl(sum(SOMNY),0),2) somny,round(nvl(sum(TAXMNY),0),2) taxmay,round(nvl(sum(SDTAXMNY),0),2) sdtaxmny,
b.addr,b.LAWMAN,round(nvl(sum(d.je),0),2) yjje,round(nvl(sum(d.se),0),2) yjse 
from tax_taxdata a,tax_company b,v_tax_taxman c,fp_yj d
where a.djxh=b.djxh and a.id=d.nsrsbh(+)
and nvl(b.TYPCLSID,&amp;apos; &amp;apos;) like &amp;apos;[%HY]%&amp;apos; 
and nvl(b.hymx_dm,&amp;apos; &amp;apos;) like &amp;apos;[%GBHY]%&amp;apos;
and nvl(b.taxman,&amp;apos; &amp;apos;) like &amp;apos;%[%TAXMAN]%&amp;apos;
and b.taxman=c.id(+) 
and to_char(d.kprq(+),&amp;apos;yyyy-mm&amp;apos;)&amp;gt;=&amp;apos;[%YYMM1]&amp;apos;
and to_char(d.kprq(+),&amp;apos;yyyy-mm&amp;apos;)&amp;lt;=&amp;apos;[%YYMM2]&amp;apos;
and yymm&amp;gt;=&amp;apos;[%YYMM1]&amp;apos; and yymm&amp;lt;=&amp;apos;[%YYMM2]&amp;apos;
and (a.id like &amp;apos;[%CORPID]%&amp;apos; or a.djxh like &amp;apos;[%CORPID]%&amp;apos; or a.name like &amp;apos;%[%CORPID]%&amp;apos;)
--and b.swjg_dm like &amp;apos;%[%SWJG_DM]%&amp;apos; and b.jdxz_dm like &amp;apos;%[%JDXZ_DM]%&amp;apos;
[%SWJG_JDXZ]
group by a.id,a.name,b.taxman,c.name,b.towns,b.addr,b.lawman
) 
where somny &amp;gt;=nvl(&amp;apos;[%SOMNY1]&amp;apos;,0) AND somny &amp;lt;=nvl(&amp;apos;[%SOMNY2]&amp;apos;,9999999999999)
  AND taxmay &amp;gt;= nvl(&amp;apos;[%TAXMANY1]&amp;apos;,0) AND taxmay &amp;lt;= nvl(&amp;apos;[%TAXMANY2]&amp;apos;,9999999999999)
order by somny desc
)</DATDSC><C4 ></C4><C5 ></C5><C6 ></C6><C7 >MAIN</C7><C8 >MAIN</C8><C9 >MAIN</C9><C10 >MAIN</C10><C11 >MAIN</C11><C12 >MAIN</C12><C13 >MAIN</C13><C14 >MAIN</C14><C15 ></C15><C16 >MAIN</C16><C17 >MAIN</C17><C18 >MAIN</C18><C19 >MAIN</C19><C20 >MAIN</C20><C21 >MAIN</C21><C22 >MAIN</C22><C23 >MAIN</C23><C24 >MAIN</C24><C25 >MAIN</C25><C26 >MAIN</C26><C27 >MAIN</C27><C28 >MAIN</C28><C29 >MAIN</C29><C30 >MAIN</C30></ROW>
