<ROW num="0" ><ID >main</ID><NAME ></NAME><DATDSC >select rownum,id,name,hj1,hj2,z,e,zzs1,xfs1,sds1,sdsw1,zzs2,xfs2,sds2,sdsw2 from (
select c.id,c.name,a.SEHJ/10000 hj1,b.SEHJ/10000 hj2,&amp;apos;&amp;apos; z,&amp;apos;&amp;apos; e,a.ZZS/10000 zzs1,a.xfs/10000 xfs1,a.sds/10000 sds1,a.sdsw/10000 sdsw1,b.ZZS/10000 zzs2,b.xfs/10000 xfs2,b.sds/10000 sds2,b.sdsw/10000 sdsw2 from
(select b.id,b.name,sum(sehj) sehj, sum(zzs) zzs,sum(xfs) xfs,sum(sds) sds,sum(sdsw) sdsw
from tax_kpdata a,tax_company b
where a.NSRSBH=b.id(+)
and b.typclsid like &amp;apos;[%HY]%&amp;apos;
and to_char(jkfsrq,&amp;apos;yyyy-mm&amp;apos;)&amp;gt;=&amp;apos;[%YYMM1]&amp;apos;
and to_char(jkfsrq,&amp;apos;yyyy-mm&amp;apos;)&amp;lt;=&amp;apos;[%YYMM2]&amp;apos;
[%SWJGSTR]
group by b.id,b.name
) a,
(select b.id,b.name,sum(sehj) sehj,sum(zzs) zzs,sum(xfs) xfs,sum(sds) sds,sum(sdsw) sdsw
from tax_kpdata a,tax_company b
where a.NSRSBH=b.id(+)
and b.typclsid like &amp;apos;[%HY]%&amp;apos;
[%SWJGSTR]
and to_char(jkfsrq,&amp;apos;yyyy-mm&amp;apos;) &amp;gt;= to_char(add_months(to_date(&amp;apos;[%YYMM1]&amp;apos;,&amp;apos;YYYY-MM&amp;apos;),-12),&amp;apos;YYYY-MM&amp;apos;)
and to_char(jkfsrq,&amp;apos;yyyy-mm&amp;apos;) &amp;lt;= to_char(add_months(to_date(&amp;apos;[%YYMM2]&amp;apos;,&amp;apos;YYYY-MM&amp;apos;),-12),&amp;apos;YYYY-MM&amp;apos;)
group by b.id,b.name
) b,
(
  select distinct b.id,b.name from tax_kpdata a,tax_company b 
  where to_char(jkfsrq,&amp;apos;yyyy-mm&amp;apos;) &amp;gt;= to_char(add_months(to_date(&amp;apos;[%YYMM1]&amp;apos;,&amp;apos;YYYY-MM&amp;apos;),-12),&amp;apos;YYYY-MM&amp;apos;)
  and to_char(jkfsrq,&amp;apos;yyyy-mm&amp;apos;)&amp;lt;=&amp;apos;[%YYMM2]&amp;apos; and b.typclsid like  &amp;apos;[%HY]%&amp;apos;
 and a.nsrsbh=b.id(+)
) c
where c.id=a.id(+) and c.id=b.id(+)
order by a.SEHJ desc
)</DATDSC><C4 >main</C4><C5 >main</C5><C6 >main</C6><C7 >main</C7><C8 >main</C8><C9 >main</C9><C10 >main</C10><C11 >main</C11><C12 >main</C12><C13 >main</C13></ROW>
