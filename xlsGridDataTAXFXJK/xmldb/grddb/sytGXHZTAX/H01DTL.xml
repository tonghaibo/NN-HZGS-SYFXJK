<ROW num="0" ><GRDOSDS_VALUE >var pub = new JavaPackage(&amp;quot;com.xlsgrid.net.pub&amp;quot;);

//替换SQL参数
function replaceParam(mwobj,request,sql)
{
var usrinfo = web.EASession.GetLoginInfo(request);
	var usrorg = usrinfo.getusrOrg();
	var usrid = usrinfo.getUsrid();
	var swjg_dm = request.getParameter(&amp;quot;swjg&amp;quot;);
	
	if(swjg_dm == &amp;quot;&amp;quot;){
	sql = pub.EAFunc.Replace(sql,&amp;quot;[%SWJGSTR]&amp;quot;,&amp;quot;and b.swjg_dm like (select decode(scope,&amp;apos;ALL&amp;apos;,&amp;apos;%&amp;apos;,substr(&amp;apos;[%SYS_DEPTID]&amp;apos;,0,7 )||&amp;apos;%&amp;apos;) from v_depts where id=&amp;apos;[%SYS_DEPTID]&amp;apos;)&amp;quot;);
	}
	sql = pub.EAFunc.Replace(sql,&amp;quot;[%SWJGSTR]&amp;quot;,&amp;quot;and b.swjg_dm like &amp;apos;&amp;quot;+swjg_dm+&amp;quot;%&amp;apos;&amp;quot;);
	//throw new Exception(sql+&amp;quot;swjg:&amp;quot;+swjg_dm);
	return sql;
}</GRDOSDS_VALUE></ROW>
</ROWSET>
