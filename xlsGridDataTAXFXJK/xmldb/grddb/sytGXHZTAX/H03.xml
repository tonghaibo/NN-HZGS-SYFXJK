<ROW num="0" ><ID >MAIN</ID><NAME ></NAME><DATDSC >select rownum,nsrsbh,name,sehj,zzs,xfs,sdsn,sdsw,grsdsse,clgzsse,xzxsf,taxman from (
select a.nsrsbh,b.name,
       sum(SEHJ) SEHJ,
       sum(zzs) zzs, 
       sum(XFS) XFS,
       sum(SDSN) SDSN,
       sum(SDSW) SDSW,
       sum(GRSDSSE) GRSDSSE,
       sum(CLGZSSE) CLGZSSE,
       sum(XZXSF) XZXSF,
       max(c.name) taxman
from tax_kpdata a,v_tax_company b,v_tax_taxman c
where a.nsrsbh=b.id(+) and b.TAXMAN=c.id(+)
  and to_char(a.JKFSRQ,&amp;apos;yyyy-mm&amp;apos;)&amp;gt;=&amp;apos;[%YYMM1]&amp;apos;
  and to_char(a.JKFSRQ,&amp;apos;yyyy-mm&amp;apos;)&amp;lt;=&amp;apos;[%YYMM2]&amp;apos;
  and (b.id like &amp;apos;%[%CMID]%&amp;apos; or b.name like &amp;apos;%[%CMID]%&amp;apos;)
  and b.typclsid like &amp;apos;[%HYCODE]%&amp;apos;
