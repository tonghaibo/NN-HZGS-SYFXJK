<ROW num="0" ><ID >main</ID><NAME ></NAME><DATDSC >select rownum rn,hycode,hynam,hj1/10000,hj2/10000,&amp;apos;&amp;apos; zje,&amp;apos;&amp;apos; zjbl,zzs1/10000,xfs1/10000,sds1/10000,sdsw1/10000,zzs2/10000,xfs2/10000,sds2/10000,sdsw2/10000 from(
select s1.hycode,s1.hynam,s1.hj1,s2.hj2,s1.zzs1,s1.xfs1,s1.sds1,s1.sdsw1,s2.zzs2,s2.xfs2,s2.sds2,s2.sdsw2 from
(
select c.hycode,c.NAME hynam,nvl(cc.hj1,0) hj1,nvl(cc.zzs,0) zzs1,nvl(cc.xfs,0) xfs1,nvl(cc.sds,0) sds1,nvl(cc.sdsw,0) sdsw1 from v_tax_hyclass c,(
select typclsid,sum(sehj) hj1,sum(zzs) zzs,sum(xfs) xfs,sum(sds) sds,sum(sdsw) sdsw from(
select b.typclsid,a.nsrsbh,b.name,zzs,xfs,sds,sdsw,sehj
from tax_kpdata a,tax_company b
where a.NSRSBH=b.id(+)
and b.swjg_dm like &amp;apos;[%SWJG]%&amp;apos;
and b.typclsid like &amp;apos;[%HY]%&amp;apos;
and to_char(jkfsrq,&amp;apos;yyyy-mm&amp;apos;)&amp;gt;=&amp;apos;[%YYMM1]&amp;apos;
and to_char(jkfsrq,&amp;apos;yyyy-mm&amp;apos;)&amp;lt;=&amp;apos;[%YYMM2]&amp;apos;
) group by typclsid
) cc where c.ID=cc.typclsid(+)
) s1,
(
select c.hycode,c.NAME hynam,nvl(cc.hj2,0) hj2,nvl(cc.zzs,0) zzs2,nvl(cc.xfs,0) xfs2,nvl(cc.sds,0) sds2,nvl(cc.sdsw,0) sdsw2 from v_tax_hyclass c,(
select typclsid,sum(sehj) hj2,sum(zzs) zzs,sum(xfs) xfs,sum(sds) sds,sum(sdsw) sdsw from(
select b.typclsid,a.nsrsbh,b.name,zzs,xfs,sds,sdsw,sehj
from tax_kpdata a,tax_company b
where a.NSRSBH=b.id(+)
and b.swjg_dm like &amp;apos;[%SWJG]%&amp;apos;
and b.typclsid like &amp;apos;[%HY]%&amp;apos;
and to_char(jkfsrq,&amp;apos;yyyy-mm&amp;apos;) &amp;gt;= to_char(add_months(to_date(&amp;apos;[%YYMM1]&amp;apos;,&amp;apos;YYYY-MM&amp;apos;),-12),&amp;apos;YYYY-MM&amp;apos;)
and to_char(jkfsrq,&amp;apos;yyyy-mm&amp;apos;) &amp;lt;= to_char(add_months(to_date(&amp;apos;[%YYMM2]&amp;apos;,&amp;apos;YYYY-MM&amp;apos;),-12),&amp;apos;YYYY-MM&amp;apos;)
) group by typclsid
) cc where c.ID=cc.typclsid(+)
) s2
where s1.hycode=s2.hycode(+)
order by hj1 desc
)</DATDSC><GRDJSDS >DSC:</GRDJSDS><C5 >main</C5><C6 >main</C6><C7 >main</C7><C8 >main</C8><C9 >main</C9><C10 >main</C10><C11 >main</C11><C12 >main</C12><C13 >main</C13><C14 >main</C14><C15 >main</C15><C16 >main</C16></ROW>
