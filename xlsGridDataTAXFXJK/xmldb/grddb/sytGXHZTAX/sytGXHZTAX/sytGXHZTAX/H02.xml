<ROWSET>
<ROW num="0" ><ID >MAIN</ID><NAME ></NAME><DATDSC >select rownum rn,hynam,nsrsbh,aa.name,sehj,zzs,xfs,sds,sdsw,bb.NAME swjg from(
select max(c.NAME) hyNam,a.nsrsbh,max(b.name) name,sum(zzs) zzs,sum(xfs) xfs,sum(sds) sds,sum(sdsw) sdsw,sum(sehj) sehj,substr(max(b.swjg_dm),0,7) swjg
from tax_kpdata a,tax_company b,v_tax_hyclass c
where a.NSRSBH=b.id(+)
and b.typclsid like &amp;apos;[%HY]%&amp;apos; 
and b.swjg_dm like &amp;apos;[%SWJG]%&amp;apos;
--and b.swjg_dm like (select decode(scope,&amp;apos;ALL&amp;apos;,&amp;apos;%&amp;apos;,&amp;apos;[%SYS_DEPTID]&amp;apos;) from v_depts where id=&amp;apos;[%SYS_DEPTID]&amp;apos;)
and b.typclsid=c.id(+)
and jkfsrq &amp;gt;= to_date(&amp;apos;[%YYMM1]&amp;apos;,&amp;apos;YYYY-MM&amp;apos;)   
and jkfsrq &amp;lt;= to_date(&amp;apos;[%YYMM2]&amp;apos;,&amp;apos;YYYY-MM&amp;apos;)
group by a.nsrsbh
order by sehj desc
) aa,v_depts bb
--where sehj&amp;gt;=500000  and aa.swjg=bb.ID(+)
where sehj &amp;gt;= nvl(&amp;apos;[%SEHJ1]&amp;apos;,0) AND sehj &amp;lt;= nvl(&amp;apos;[%SEHJ2]&amp;apos;,9999999999999) 
and aa.swjg=bb.ID(+)
</DATDSC><C4 >MAIN</C4><C5 >MAIN</C5><C6 >MAIN</C6><C7 >MAIN</C7><C8 >MAIN</C8><C9 >MAIN</C9><C10 >MAIN</C10><C11 >MAIN</C11><C12 >MAIN</C12><C13 >MAIN</C13><C14 >MAIN</C14><C15 >MAIN</C15></ROW>
</ROWSET>
