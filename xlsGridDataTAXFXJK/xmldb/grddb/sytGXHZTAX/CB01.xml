<ROW num="0" ><ID >main</ID><NAME ></NAME><DATDSC >select s.name, zz,sd,zn,fk,qt from v_swjg s,(
select swjg,sum(ZZS)/10000 zz,sum(SDS)/10000 sd,sum(ZNJ)/10000 zn,sum(FK)/10000 fk,sum(QT)/10000 qt from (
select substr(swjg_dm,0,7) swjg,ZZS,SDS,ZNJ,FK,QT from (
select USRID,USRNAM,OWNER,HY,ZZS,SDS,ZNJ,FK,QT,SDDAT,RKDAT,BILNO,RES,NOTE 
from tax_pshdr a,TAX_PSDTL b 
where rkdat&amp;gt;=decode(&amp;apos;[%RKDAT1]&amp;apos;,&amp;apos;&amp;apos;,&amp;apos;0&amp;apos;,&amp;apos;[%RKDAT1]&amp;apos;)
  and rkdat&amp;lt;=decode(&amp;apos;[%RKDAT2]&amp;apos;,&amp;apos;&amp;apos;,&amp;apos;9999999999&amp;apos;,&amp;apos;[%RKDAT2]&amp;apos;)
  and a.guid=b.formguid
order by RKDAT,USRID  
) p,v_tax_company cm
where p.usrid=cm.id(+)
) 
group by swjg
) ps
where s.id=ps.swjg(+)
order by s.id</DATDSC><C4 >main</C4></ROW>
</ROWSET>
