function replaceParam(mwobj,request,sql)
{
	var hy = pub.EAFunc.NVL(request.getParameter(&amp;quot;TYPCLSID&amp;quot;),&amp;quot;&amp;quot;);
	if (hy != &amp;quot;&amp;quot;) {
		sql = pub.EAFunc.Replace(sql,&amp;quot;[%TYPCLSID]&amp;quot;,&amp;quot;and b.typclsid like &amp;apos;&amp;quot;+hy+&amp;quot;%&amp;apos;&amp;quot;);
	}
	return sql;
}
