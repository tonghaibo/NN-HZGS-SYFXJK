<ROW num="0" ><ID >MAIN</ID><NAME ></NAME><DATDSC >select rownum,id,name,lawman,PWDEPT,ammno,eleqty,taxman,town,addr from (
select * from (
select b.id,nvl(b.ammno,a.usrid) ammno,sum(nvl(a.eleqty,0)) eleqty,b.taxman,b.lawman,nvl(b.name,a.usrnam) name,b.addr,PWDEPT,town
from tax_eledata a,tax_company b 
where a.usrid=b.ammno(+)
  and a.yymm&amp;gt;=&amp;apos;[%YYMM1]&amp;apos; 
  and a.yymm&amp;lt;=&amp;apos;[%YYMM2]&amp;apos;
  and (a.usrid like &amp;apos;[%CMID]%&amp;apos; or a.usrnam like &amp;apos;%[%CMID]%&amp;apos; or b.id like &amp;apos;[%CMID]%&amp;apos; or b.name like &amp;apos;%[%CMID]%&amp;apos;)
  and a.typ like &amp;apos;[%TYP]%&amp;apos;
  --and nvl(b.stat(+),&amp;apos; &amp;apos;) &amp;lt;&amp;gt; &amp;apos;注销&amp;apos;
  --and b.typclsid(+) like &amp;apos;[%TYPCLSID]%&amp;apos;
  [%TYPCLSID]
group by nvl(b.ammno,a.usrid),taxman,lawman,nvl(b.name,a.usrnam),addr,b.id,PWDEPT,town
) a 
where a.eleqty&amp;gt;=decode(&amp;apos;[%ELEQTY1]&amp;apos;,&amp;apos;&amp;apos;,0,&amp;apos;[%ELEQTY1]&amp;apos;)
