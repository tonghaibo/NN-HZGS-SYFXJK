<ROWSET>
<ROW num="0" ><ID >MAIN</ID><NAME ></NAME><DATDSC >select rownum,id,name,lawman,somny,taxmay,sdtaxmny,taxman,towns,addr from (
select id,name,taxman,somny,taxmay,sdtaxmny,towns,addr,lawman from(
select a.id,a.name,b.taxman,round(nvl(sum(SOMNY),0),2) somny,round(nvl(sum(TAXMNY),0),2) taxmay,round(nvl(sum(SDTAXMNY),0),2) sdtaxmny,
b.towns,b.addr,b.LAWMAN 
from tax_taxdata a,v_tax_company b
where a.id=b.id and b.TYPCLSID like &amp;apos;[%HY]%&amp;apos; 
--and nvl(b.stat,&amp;apos; &amp;apos;) &amp;lt;&amp;gt; &amp;apos;注销&amp;apos;
and yymm&amp;gt;=&amp;apos;[%YYMM1]&amp;apos; and yymm&amp;lt;=&amp;apos;[%YYMM2]&amp;apos;
and (a.id like &amp;apos;[%CORPID]%&amp;apos; or a.name like &amp;apos;%[%CORPID]%&amp;apos;)
group by a.id,a.name,b.taxman,b.towns,b.addr,b.lawman
) 
where somny &amp;gt;=nvl(&amp;apos;[%SOMNY1]&amp;apos;,0) AND somny &amp;lt;=nvl(&amp;apos;[%SOMNY2]&amp;apos;,9999999999999)
  AND taxmay &amp;gt;= nvl(&amp;apos;[%TAXMANY1]&amp;apos;,0) AND taxmay &amp;lt;= nvl(&amp;apos;[%TAXMANY2]&amp;apos;,9999999999999)
order by somny desc
)</DATDSC><C4 ></C4><C5 ></C5><C6 ></C6><C7 >MAIN</C7><C8 >MAIN</C8><C9 >MAIN</C9><C10 >MAIN</C10><C11 >MAIN</C11><C12 >MAIN</C12><C13 >MAIN</C13><C14 >MAIN</C14><C15 ></C15><C16 >MAIN</C16><C17 >MAIN</C17><C18 >MAIN</C18><C19 >MAIN</C19><C20 >MAIN</C20></ROW>
