select nvl((select max(name) from v_tax_hyclass where id like &amp;apos;[%HY]&amp;apos;),&amp;apos;所有&amp;apos;) name from dual  
</DATDSC><C4 >GRDDSCDS_1</C4><C5 >GETCORPNAM</C5><C6 >GETCORPNAM</C6><C7 ></C7><C8 ></C8><C9 ></C9><C10 ></C10><C11 ></C11><C12 ></C12><C13 ></C13><C14 ></C14><C15 >GETCORPNAM</C15><C16 ></C16><C17 ></C17><C18 ></C18><C19 ></C19><C20 ></C20></ROW>
