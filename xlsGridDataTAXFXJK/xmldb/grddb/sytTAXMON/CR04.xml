<ROW num="0" ><ID >MAIN</ID><NAME ></NAME><DATDSC >select &amp;apos;&amp;apos; flag,a.id,a.name,a.addr,a.somny,a.taxmny,
	round(decode(somny,0,&amp;apos;0&amp;apos;,taxmny/somny),3)*100 taxbers ,
       a.bears bears,
       (round(decode(somny,0,&amp;apos;0&amp;apos;,taxmny/somny),3)-a.bears/100)*100 cy,
       a.taxman 
from (
select b.Id,b.name,max(b.addr) addr,nvl(sum(a.somny),0) somny,nvl(sum(a.taxmny),0) taxmny,
  nvl(&amp;apos;&amp;apos;,nvl(c.tax,0)) bears,b.taxman taxman 
from tax_taxdata a,v_tax_company b,TAX_COMPCLASS c
where a.id=b.id and b.typclsid=c.id
  --and b.stat != &amp;apos;注销&amp;apos;
  and b.typclsid = &amp;apos;[%CORPID]&amp;apos;
  and a.yymm&amp;gt;=&amp;apos;[%YYMM1]&amp;apos;
  and a.yymm&amp;lt;=&amp;apos;[%YYMM2]&amp;apos;
   and nvl(b.towns,&amp;apos; &amp;apos;) IN (select name from V_TAX_TOWNS WHERE ID like &amp;apos;[%TOWN]%&amp;apos;)
  group by b.Id,b.name,b.taxman,nvl(&amp;apos;[%TAXBEAR]&amp;apos;,nvl(c.tax,0))
) a
 --where a.taxbers-a.bears&amp;gt;0.05 or somny=0
 where round(decode(somny,0,&amp;apos;0&amp;apos;,taxmny/somny),3) &amp;lt; a.bears/100
