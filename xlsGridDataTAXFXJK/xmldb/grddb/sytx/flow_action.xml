//数据加载完毕
var obj= window.dialogArguments;
function _thisOnpost_loaddata(sheet)
{
	var str=obj.str;
	for(var i=0;i&amp;lt;str.length;i++){
		if(str[i]!=&amp;quot;&amp;quot;){
			var a=str[i].split(&amp;quot;-&amp;quot;);
			var id=a[0];
			var name=a[1];
			_this.SetCellText(0,i+1,1,id);
			_this.SetCellText(0,i+1,2,name);
		}
	}
}

//鼠标双击
function _thisOnMouseDClick(sheet,row,col)
{
	if(sheet==0&amp;&amp;row&amp;gt;=1&amp;&amp;col&amp;gt;=1)
	{
		var rt=new Object();
		rt.id=_this.GetCellText(sheet,row,1);
		rt.name=_this.GetCellText(sheet,row,2);
		window.returnValue =rt;
		window.close();
	}
}
