//数据加载完毕
function _thisOnpost_loaddata(sheet)
{
  	var str52 = _this.GetCellText(sheet,6,1);
	_this.SetToCodeEditor(&amp;quot;&amp;quot;,0,6,1,&amp;quot;&amp;quot;,&amp;quot;&amp;quot;,&amp;quot;&amp;quot;);
	_this.SetCellText(sheet,6,1,str52);
	_this.SetColVisible(sheet,7,-1);
	
	for (var i=_this.GetMainCellRangeRow1(sheet);i&amp;lt;=_this.GetMainCellRangeRow2(sheet);i++){
		_this.SetCellText(sheet,i,8,&amp;quot;点击编辑&amp;quot;);
	}

}

//鼠标左键点击
function _thisOnMouseLClick(sheet,row,col)
{
	if ((row &amp;gt;= _this.GetMainCellRangeRow1(sheet) &amp;&amp; row &amp;lt;= _this.GetMainCellRangeRow2(sheet)) &amp;&amp; col == 8){
		var urlguid = _this.GetCellText(sheet,row,7);
		window.open(&amp;quot;show.sp?grdid=URLBODY&amp;urlguid=&amp;quot;+urlguid);
	}  

  
}
