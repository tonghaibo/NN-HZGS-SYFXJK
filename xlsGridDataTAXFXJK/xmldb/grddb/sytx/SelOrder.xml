//双击选择主数据区行
function _thisOnDblClickMainCellRangeRow(sheet,row)
{
	var r1 = _this.GetMainCellRangeRow1(sheet);
	var r2 = _this.GetMainCellRangeRow2(sheet);
	var ordid = _this.GetCellText(sheet,row,1);
	for (var r = r1;r&amp;lt;=r2;r++)
	{
		var tmpord = _this.GetCellText(sheet,r,1);
		if (r!=row &amp;&amp; tmpord==ordid)
		{
			_this.SetRowVisible(sheet,r,-1);
		}
	}
}
