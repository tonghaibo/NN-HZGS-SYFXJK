//鼠标双击
function _thisOnMouseDClick(sheet,row,col)
{
	if ( sheet == 0 &amp;&amp; row &amp;gt;=_this.GetMainCellRangeRow1(0) &amp;&amp; col == 7) {
		var osfunc = _this.GetCellText(sheet,row,col) ;
		if ( osfunc==&amp;quot;&amp;quot; ) {
			osfunc  = _this.GetCellText(sheet,row,1);
			_this.SetCellText(sheet,row,col,osfunc  ) ;
		}
		openWindow(&amp;quot;xlsgrid/jsp/design/designer.jsp?sytid=&amp;quot;+_this.GetCellText(0,3,2)+&amp;quot;&amp;grdid=VAPPOS&amp;opensheet=2&amp;addosfunc=&amp;quot;+_this.GetCellText(0,5,2)+&amp;quot;_&amp;quot;+osfunc + &amp;quot;&amp;addosfuncnote=param:  1) ds:logininfo 2)request 3) ssoid 4)loginurl 5) pageurl  return:pageurl&amp;quot; ) ;//+ &amp;quot;&amp;addosfuncnote=param：1）ds:；2）request；3）ssoid 4) loginurl 登录的URL 5) pageurl 登录的页面  返回：登录的页面地址&amp;quot;
	}
	if ( sheet == 0 &amp;&amp; row &amp;gt;=_this.GetMainCellRangeRow1(0) &amp;&amp; col == 4) {
		openWindow(&amp;quot;show.sp?grdid=WS&amp;quot;);
	}
	if ( sheet == 0 &amp;&amp; row &amp;gt;=_this.GetMainCellRangeRow1(0) &amp;&amp; col == 5) {
		openWindow(&amp;quot;show.sp?grdid=ETL&amp;quot;);
	}	
}
