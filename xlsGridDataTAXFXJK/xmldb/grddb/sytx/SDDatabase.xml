<ROW num="0" ><ID >MAIN</ID><NAME ></NAME><DATDSC >select id,name from (
select a.table_name id,b.comments name,&amp;apos;TABLE&amp;apos; typ from user_tables a, user_tab_comments b
 where a.table_name=b.table_name(+) and a.table_name like &amp;apos;[%TABLE_NAME]%&amp;apos;
union 
select a.view_name id,b.comments name, &amp;apos;VIEW&amp;apos; typ from user_views a, user_tab_comments b
 where a.view_name=b.table_name(+) and a.view_name like &amp;apos;[%TABLE_NAME]%&amp;apos;
)
where typ like &amp;apos;[%TYP]%&amp;apos;
order by typ,id</DATDSC></ROW>
