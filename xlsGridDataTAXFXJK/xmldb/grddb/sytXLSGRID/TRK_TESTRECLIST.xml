<ROWSET>
<ROW num="0" ><ID >DETAIL</ID><NAME ></NAME><DATDSC >SELECT 	GUID,
	STAT,
	(SELECT NAME FROM V_PRJ WHERE ID=PRJ) 所属项目,
	TESDAT 实施日期,
	(SELECT NAME FROM V_USR9 WHERE ID=IMPUSER) 实施人,
	CORP 实施地点,
	TITLE 内容,
	NOTE 说明,
	SUM 总计,
	CASE STAT WHEN 1 THEN &amp;apos;录入状态&amp;apos; 
	WHEN 2 THEN &amp;apos;客户确认&amp;apos;
	WHEN 3 THEN &amp;apos;实施完毕&amp;apos;
	end

FROM TRK_TESTSYSRECHL
WHERE IMPUSER LIKE&amp;apos;[%USERID]%&amp;apos;AND 
	PRJ LIKE&amp;apos;[%PRJ]%&amp;apos;AND 
	TESDAT &amp;gt;=to_date(&amp;apos;[%FROMDAT]&amp;apos;,&amp;apos;YYYY-MM-DD&amp;apos;)AND 
	TESDAT&amp;lt;=to_date(&amp;apos;[%TODAT]&amp;apos;,&amp;apos;YYYY-MM-DD&amp;apos;)AND 
	TITLE LIKE &amp;apos;[%TITLE]%&amp;apos;</DATDSC><C4 >DETAIL</C4><C5 >DETAIL</C5></ROW>
</ROWSET>
