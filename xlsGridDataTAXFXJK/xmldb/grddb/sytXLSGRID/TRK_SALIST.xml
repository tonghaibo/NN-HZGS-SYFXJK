<ROW num="0" ><ID >DETAIL</ID><NAME ></NAME><DATDSC >select 
	dat,
	name 标题,
	note ,
	sausr 提出人,
	(select name from v_usr9 where id=fixuser) 处理人,
	decode(testdat,null,&amp;apos;&amp;apos;,&amp;apos;完成&amp;apos;) 开发状态,
	testdat 开发时间,
	edupart 参与人,
	edudat 培训时间,
	decode(impdat ,null,&amp;apos;&amp;apos;,&amp;apos;完成&amp;apos;) 实施状态,
	impdat 实施时间,
	case stat 
       when &amp;apos;1&amp;apos; then &amp;apos;录入状态&amp;apos;
	when &amp;apos;2&amp;apos; then &amp;apos;需求确认&amp;apos;
	when &amp;apos;3&amp;apos; then &amp;apos;设计完成&amp;apos;
	when &amp;apos;4&amp;apos; then &amp;apos;开发完成&amp;apos;
	when &amp;apos;5&amp;apos; then &amp;apos;测试通过&amp;apos;
	when &amp;apos;6&amp;apos; then &amp;apos;操作手册已提交&amp;apos;
	when &amp;apos;7&amp;apos; then &amp;apos;培训完毕&amp;apos;
	when &amp;apos;8&amp;apos; then &amp;apos;实施完毕&amp;apos;
	when &amp;apos;9&amp;apos; then &amp;apos;强制取消&amp;apos;
  	end,
  	&amp;apos;&amp;apos;,
	guid,
	stat
from TRK_SAMODIFY 
where   PRJ like &amp;apos;[%PRJ]%&amp;apos; and
	dat &amp;lt;= to_date(&amp;apos;[%TOSACRTDAT]&amp;apos;,&amp;apos;YYYY-MM-DD&amp;apos;) and
	dat &amp;gt;= to_date(&amp;apos;[%FROSACRTDAT]&amp;apos;,&amp;apos;YYYY-MM-DD&amp;apos;) 
order by dat	</DATDSC><C4 >DETAIL</C4><C5 >DETAIL</C5><C6 >DETAIL</C6><C7 >DETAIL</C7><C8 >DETAIL</C8><C9 >DETAIL</C9><C10 >DETAIL</C10><C11 >DETAIL</C11><C12 >DETAIL</C12><C13 >DETAIL</C13><C14 >DETAIL</C14><C15 >DETAIL</C15><C16 >DETAIL</C16><C17 >DETAIL</C17><C18 >DETAIL</C18><C19 >DETAIL</C19><C20 >DETAIL</C20><C21 >DETAIL</C21><C22 >DETAIL</C22><C23 >DETAIL</C23><C24 >DETAIL</C24><C25 >DETAIL</C25><C26 >DETAIL</C26><C27 >DETAIL</C27><C28 >DETAIL</C28><C29 >DETAIL</C29><C30 >DETAIL</C30></ROW>
